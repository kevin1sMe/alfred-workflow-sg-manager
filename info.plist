				<string># --- 捕获传递给当前脚本的参数 ---
# 确保用双引号包围变量，以正确处理包含空格或其他特殊字符的参数
ACTION_KEY_VAL="$1"
CUR_VALUE_VAL="$2"

# --- 调试日志 (您可以根据需要保留或移除) ---
env &gt;&gt; /tmp/alfred-frp.log
echo "`date` args to this script: $@" &gt;&gt; /tmp/alfred-frp.log
echo "`date` \$1 captured: '${ACTION_KEY_VAL}'" &gt;&gt; /tmp/alfred-frp.log # 打印捕获到的 $1
echo "`date` \$2 captured: '${CUR_VALUE_VAL}'" &gt;&gt; /tmp/alfred-frp.log # 打印捕获到的 $2
# 注意：下面的 env 会显示脚本内部设置的环境变量，但这不影响传给后续 Alfred 组件的方式
# export action_key="$ACTION_KEY_VAL" # 这一行对于传给后续Alfred组件不是必需的
# export cur_value="$CUR_VALUE_VAL"   # 这一行对于传给后续Alfred组件不是必需的
# env &gt;&gt; /tmp/alfred-frp.log


# --- 输出 Alfred 需要的 JSON 格式，包含要传递的变量 ---
# 使用 jq 工具构建 JSON
# -n: 从空输入开始构建
# --arg: 将 bash 变量传递给 jq 过滤器
jq -n \
  --arg ak "$ACTION_KEY_VAL" \
  --arg cv "$CUR_VALUE_VAL" \
  '{ alfredworkflow: { variables: { action_key: $ak, cur_value: $cv } } }'

# --- 移除原脚本中不用于传递变量的输出行 ---
# echo -n $cur_value # 移除此行，因为它只是将 $cur_value 的值输出，Alfred 不会解析为变量

exit 0 # 脚本成功退出</string>
